{
  "name": "chat-monkey",
  "version": "1.0.1",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings=0",
    "format:check": "prettier --check .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "ci": "npm run lint:check && npm run typecheck && npm run format:check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "db:generate": "drizzle-kit generate --out ./drizzle",
    "db:migrate": "drizzle-kit migrate --out ./drizzle",
    "postinstall": "electron-builder install-app-deps",
    "build:win:x64": "rimraf dist && npm run build && electron-builder --win nsis --x64 --publish=never",
    "build:mac:arm64": "rimraf dist && npm run build && electron-builder --mac dmg --arm64 --publish=never",
    "build:mac:x64": "rimraf dist && npm run build && electron-builder --mac dmg --x64 --publish=never",
    "build:linux:appimage": "rimraf dist && npm run build && electron-builder --linux AppImage --x64 --publish=never",
    "build:linux:deb": "rimraf dist && npm run build && electron-builder --linux deb --x64 --publish=never",
    "prepare": "husky",
    "icon": "npx electron-icon-builder --input=resources/icon.png --output=build --flatten"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.23",
    "@ai-sdk/react": "^2.0.30",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@hono/node-server": "^1.19.1",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "ai": "^5.0.30",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "electron-store": "8.1.0",
    "electron-updater": "^6.1.7",
    "embla-carousel-react": "^8.6.0",
    "gsap": "^3.13.0",
    "hono": "^4.9.6",
    "lucide-react": "^0.542.0",
    "ogl": "^1.0.11",
    "react-resizable-panels": "^3.0.1",
    "react-syntax-highlighter": "^15.6.6",
    "streamdown": "^1.1.10",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^4",
    "zustand": "^4.5.0"
  },
  "drizzle-kit": {
    "schema": "./src/main/db/schema.ts",
    "out": "./drizzle",
    "dialect": "sqlite"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "electron": "^36.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "shadcn-ui": "^0.8.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.2",
    "vite": "^5.0.11"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,md,json,html}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
